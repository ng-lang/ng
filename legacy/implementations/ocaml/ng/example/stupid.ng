fun fact x i = if x == 0 then i else fact (x - 1) (x * i);

val result = fact 5 1;

fun plus1 x = x + 1;
fun times2 x = x * 2;

fun compose f g = fun x -> f (g x);

val v = (compose plus1 times2) 3;

val (x, y) = (1, 2);

val [a, b...] = [1, 2, 3];

val [c, d] = b;

val m = if x == 1 then 1 else 10;
